<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
        <link rel="stylesheet" href="/public/css/bootstrap/css/bootstrap.css">
		<link rel="stylesheet" href="/public/css/bootstrap/css/bootstrap.min.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
        <link rel="stylesheet" href="/public/css/adminlte.min.css">
        <link rel="stylesheet" href="/public/css/app.min.css">
        <title>TEmma</title>
        <link rel="icon" type="image/x-icon" href="/" />
    </head>
    <body class="hold-transition skin-purple sidebar-mini">

        <header class="main-header">
            <!-- Logo -->
            <a class="logo" href="/">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b class="logo-mini-bold" >T</b>E</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg">
                    <b class="logo-mini-bold" >T</b>Emma
                    {#<img src="/public/img/TEmma.svg">#}
                </span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <div class="col-md-12">
                    <!-- Sidebar toggle button-->
                    <a role="button" class="sidebar-toggle" onclick="toggleBodyClass();">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </a>
                    <ul class="">
                        <li class="dropdown user user-menu" >
                            <!-- search form -->
                            <div class="col-md-3"  style="float: right;">
                                <form class="sidebar-form" method="get" action="/" >
                                    <div class="input-group">
                                        <input type="text" placeholder="Search..." class="form-control" name="searchterm">
                                        <span class="input-group-btn">
                                            <button class="btn btn-flat" id="search-btn"  type="submit"><span class="fa-search"></span>
                                            </button>
                                        </span>
                                    </div>
                                </form>
                            </div>
                            <!-- /.search form -->
                        </li>
                        <!-- Control Sidebar Toggle Button -->

                    </ul>
                </div>
            </nav>
        </header>

        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar" id="aside">

            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar" style="height: auto;">
                <!-- Sidebar Menu -->
                <ul class="sidebar-menu">
                    <li class="header">TEmma</li>
                    <!-- Optionally, you can add icons to the links -->
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-map"></i>
                            <span>Artikel</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li class="treeview">
                                <a href="/artikel">
                                    <span>index</span>
                                </a>
                            </li>
                            <li class="treeview">
                                <a href="/artikel/new">
                                    <span>create</span>
                                </a>
                            </li>
                        </ul>

                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-map"></i>
                            <span>Kunden</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li class="treeview">
                                <a href="/kunde">
                                    <span>index</span>
                                </a>
                            </li>
                            <li class="treeview">
                                <a href="/kunde/new">
                                    <span>create</span>
                                </a>
                            </li>
                        </ul>

                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-map"></i>
                            <span>Mitarbeiter</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li class="treeview">
                                <a href="/mitarbeiter">
                                    <span>index</span>
                                </a>
                            </li>
                            <li class="treeview">
                                <a href="/mitarbeiter/new">
                                    <span>create</span>
                                </a>
                            </li>
                        </ul>

                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-map"></i>
                            <span>Geschäfte</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li class="treeview">
                                <a href="/geschaefte">
                                    <span>index</span>
                                </a>
                            </li>
                            <li class="treeview">
                                <a href="/geschaefte/new">
                                    <span>create</span>
                                </a>
                            </li>
                        </ul>

                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-map"></i>
                            <span>Geschäftsart</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li class="treeview">
                                <a href="/geschaeftsart">
                                    <span>index</span>
                                </a>
                            </li>
                            <li class="treeview">
                                <a href="/geschaeftsart/new">
                                    <span>create</span>
                                </a>
                            </li>
                        </ul>

                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-map"></i>
                            <span>Posten</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li class="treeview">
                                <a href="/posten">
                                    <span>index</span>
                                </a>
                            </li>
                            <li class="treeview">
                                <a href="/posten/new">
                                    <span>create</span>
                                </a>
                            </li>
                        </ul>

                    </li>
                </ul>
                <!-- /.sidebar-menu -->
            </section>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Main content -->
            <section class="content">
			<div class="container">
            {% block body %}{% endblock %}
			</div>
        </section>
        <!-- /.content -->
    </div>
    <!-- Control Sidebar -->
    <aside class="control-sidebar control-sidebar-dark">
        <!-- Create the tabs -->
        <ul class="nav nav-tabs nav-justified control-sidebar-tabs">
            <li class="active"><a href="#control-sidebar-home-tab" data-toggle="tab"><i class="fa fa-home"></i></a></li>
            <li><a href="#control-sidebar-settings-tab" data-toggle="tab"><i class="fa fa-gears"></i></a></li>
        </ul>
        <!-- Tab panes -->
        <!-- Stats tab content -->
    </aside>

{% block stylesheets %}{% endblock %}
{% block javascripts %}{% endblock %}
<script>
    function toggleBodyClass() {
        document.body.classList.toggle("sidebar-collapse");
    }
</script>
<script src="/public/js/vendor.js"></script>
<script src="/public/js/app.js"></script>
</body>
</html>
