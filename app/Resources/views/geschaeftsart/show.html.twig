{% extends 'base.html.twig' %}

{% block body %}
    <h1>Geschaeftsart</h1>

    <table>
        <tbody>
            <tr>
                <th>Artbezeichnung</th>
                <td>{{ geschaeftsart.artbezeichnung }}</td>
            </tr>
            <tr>
                <th>Artid</th>
                <td>{{ geschaeftsart.artid }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('geschaeftsart_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('geschaeftsart_edit', { 'artid': geschaeftsart.artid }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
