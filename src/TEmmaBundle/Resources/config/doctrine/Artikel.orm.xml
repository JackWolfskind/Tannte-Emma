<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TEmmaBundle\Entity\Artikel" table="Artikel">
    <id name="artikelnr" type="integer" column="artikelNR">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="artikelbezeichnung" type="string" column="artikelBezeichnung" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="artikelpreis" type="integer" column="artikelPreis" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="artikelbestand" type="integer" column="artikelBestand" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
