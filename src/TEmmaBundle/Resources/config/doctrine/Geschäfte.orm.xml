<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TEmmaBundle\Entity\Geschäfte" table="Geschäfte">
    <indexes>
      <index name="fk_Geschäfte_Kunde_idx" columns="Kunde_kundeNR"/>
      <index name="fk_Geschäfte_Geschäftsart1_idx" columns="Geschäftsart"/>
      <index name="fk_Geschäfte_Mitarbeiter1_idx" columns="angelegtVonMitarbeiter"/>
    </indexes>
    <id name="geschäftid" type="integer" column="geschäftID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="geschäftecol" type="string" column="Geschäftecol" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="datum" type="string" column="Datum" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="geschäftsart" target-entity="Geschäftsart" fetch="LAZY">
      <join-columns>
        <join-column name="Geschäftsart" referenced-column-name="ArtID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="kundeKundenr" target-entity="Kunde" fetch="LAZY">
      <join-columns>
        <join-column name="Kunde_kundeNR" referenced-column-name="kundeNR"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="angelegtvonmitarbeiter" target-entity="Mitarbeiter" fetch="LAZY">
      <join-columns>
        <join-column name="angelegtVonMitarbeiter" referenced-column-name="mitarbeiterNR"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
